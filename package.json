{
  "name": "aigen",
  "version": "0.2.0",
  "type": "module",
  "bin": {
    "aigen": "dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
  "dev": "tsx src/index.ts --dev --out-dir out",
  "detect:env": "tsx src/detectEnv.ts",
    "test": "vitest run",
    "prepublishOnly": "tsup src/index.ts --format esm --dts",
    "gen:api": "typedoc --entryPoints src/index.ts --tsconfig tsconfig.json --plugin typedoc-plugin-markdown --out website/docs/api && node scripts/fix-typedoc-index.cjs",
    "docs:install": "cd website && pnpm install",
    "docs:start": "cd website && pnpm run start",
    "docs:build": "cd website && pnpm run build",
    "docs:serve": "cd website && pnpm run serve",
    "docs:gen": "cd website && pnpm run gen:api",
    "tools:fetch": "tsx scripts/fetch-tools.ts",
    "tools:fetch:ci": "pnpm -s dlx tsx scripts/fetch-tools.ts",
    "list:eslint-rules": "node scripts/list-eslint-rules.mjs",
    "docs:gen-styleguide": "node scripts/schema-to-markdown.mjs",
    "docs:gen-agent-instructions": "node scripts/schema-to-agent-instructions.mjs",
    "test:render": "tsx scripts/test-render.ts"
  },
  "prepare": "husky install",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "commander": "^12.1.0",
    "conf": "^13.1.0",
    "cosmiconfig": "^9.0.0",
    "enquirer": "^2.4.1",
    "eta": "^3.5.0",
    "fuse.js": "^6.6.2",
    "kleur": "^4.1.5",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "mdast-util-to-string": "^2.0.0",
    "remark-parse": "^10.0.2",
    "remark-stringify": "^10.0.3",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "unified": "^10.1.2",
    "vitest": "^2.1.9"
  }
}
